{
  "name": "linchpinstudios/theme",
  "description": "Theme will help you organize your themes inside Laravel projects easily and maintain its related assets, layouts and partials for the theme in single directory.",
  "keywords": ["laravel", "theme", "layout", "asset", "widget", "linchpin"],
  "homepage": "https://github.com/linchpinstudios/laravel-theme",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Teepluss",
      "email": "admin@laravel.in.th"
    },
    {
      "name": "Josh Hagel",
      "email": "joshhagel@linchpinstudios.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "twig/twig": "1.x"
  },
  "require-dev": {
    "illuminate/support": ">=5.0.1"
  },
  "autoload": {
    "psr-4": {
      "Linchpinstudios\\Theme\\": "src/"
    },
    "classmap": [
      "src/Exceptions.php"
    ],
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev"
}
